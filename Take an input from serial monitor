// Integer value

void setup ()
{
Serial.begin(9600);
int i=Serial.parseInt ();
Serial.println(i);
}

void loop ()
{
}

// Float value 

void setup ()
{
Serial.begin(9600);
float i=Serial.parseFloat ();
Serial.println(i);
}

void loop ()
{
}

// Character value

void setup ()
{
Serial.begin(9600);
char i=Serial.read ();
Serial.println(i);
}

void loop ()
{
}

// String value 

void setup ()
{
Serial.begin(9600);
String i=Serial.readString ();
Serial.println(i);
}

void loop ()
{
}
