// print the numbers from 1-100
void setup ()
{
Serial.begin(9600);
for(int i=1;i<=100;i++)
{
Serial.println(i);
}
}
void loop ()
{
}

//Find the odd numbers between 1-100
void setup ()
{
Serial.begin(9600);
for(int i=1;i<=100;i=i+2)
{
Serial.println(i);
}
}
void loop ()
{
}

//Find the even numbers between 1-100
void setup ()
{
Serial.begin(9600);
for(int i=2;i<=100;i=i+2)
{
Serial.println(i);
}
}
void loop ()
{
}
